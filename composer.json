{
    "name": "drupol/eulogin-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for Symfony 4 and 5, providing authentication against European Commission authentication service.",
    "license": "MIT",
    "require": {
        "php": ">= 7.1.3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "drupol/cas-bundle": "dev-master"
    },
    "require-dev": {
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "phpspec/phpspec": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "drupol\\EuloginBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\drupol\\EuloginBundle\\": "./spec/drupol/EuloginBundle/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
